<div class="content">
<h2>会員一覧</h2>
<table>
    <tr>
        <td>会員ID</td>
        <td>名前</td>
        <td>メールアドレス</td>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
    </tr>
    <% end %>
</table>
</div>